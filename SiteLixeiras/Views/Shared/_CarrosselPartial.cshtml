@model List<SiteLixeiras.Models.Foto>

<div id="carouselFotoProdutos" class="carousel slide" data-bs-ride="carousel" data-bs-interval="2000">
    <div class="carousel-inner">
        @for (int i = 0; i < Model.Count; i++)
        {
            <div class="carousel-item @(i == 0 ? "active" : "")">
                <img class="d-block mx-auto img-fluid" src="@Model[i].Url" alt="Foto do Produto" style="height:400px;">
            </div>
        }
    </div>
</div>
